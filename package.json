{
  "name": "jx",
  "version": "1.0.0",
  "description": "Manipulate JSON from command line: extend, override.",
  "main": "lib/index.js",
  "types": "lib/",
  "files": [
    "lib",
    "src"
  ],
  "bin": {
    "jx": "lib/index.js"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest --verbose --coverage --no-cache",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivandotv/jx.git"
  },
  "keywords": [
    "json",
    "cli",
    "deep",
    "extend",
    "deep-extend",
    "merge",
    "deep-merge",
    "override",
    "deep-override"
  ],
  "author": "Ivan Vlatkovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivandotv/jx/issues"
  },
  "homepage": "https://github.com/ivandotv/jx#readme",
  "jest": {
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/deep-extend": "^0.4.31",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@types/set-value": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "jest-watch-typeahead": "^0.4.0",
    "standardx": "^5.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "deep-extend": "^0.6.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "set-value": "^3.0.1",
    "undefsafe": "^2.0.2"
  }
}
